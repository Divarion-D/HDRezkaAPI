{
  "openapi": "3.0.2",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "http://127.0.1.1:8000",
      "description": "Development server"
    }
  ],
  "paths": {
    "/content/search/": {
      "get": {
        "summary": "Поиск",
        "operationId": "search_content_search__get",
        "description": "Поиск по названию",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Query",
              "type": "string"
            },
            "name": "query",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Page",
              "type": "integer",
              "default": 1
            },
            "name": "page",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Data"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/content/details/": {
      "get": {
        "summary": "Get Concrete Details",
        "operationId": "get_concrete_content_details__get",
        "description": "Get detailed information by link or id",
        "parameters": [
          {
            "required": false,
            "schema": {
              "title": "Url",
              "type": "string"
            },
            "name": "url",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Id",
              "type": "integer"
            },
            "name": "id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Concrete"
                }
              }
            }
          }
        }
      }
    },
    "/content/translations/": {
      "get": {
        "summary": "Get Content Translations",
        "operationId": "get_content_translations_content_translations__get",
        "description": "Get all translations by link or id",
        "parameters": [
          {
            "required": false,
            "schema": {
              "title": "Url",
              "type": "string"
            },
            "name": "url",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Id",
              "type": "integer"
            },
            "name": "id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Translations"
                }
              }
            }
          }
        }
      }
    },
    "/content/movie/videos/": {
      "get": {
        "summary": "Get Movie Videos",
        "operationId": "get_movie_videos_content_movie_videos__get",
        "description": "Get video file url by link or id",
        "parameters": [
          {
            "required": false,
            "schema": {
              "title": "Url",
              "type": "string"
            },
            "name": "url",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Id",
              "type": "integer"
            },
            "name": "id",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Translation Id",
              "type": "string"
            },
            "name": "translation_id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MediaFile"
                }
              }
            }
          }
        }
      }
    },
    "/content/tv_series/seasons/": {
      "get": {
        "summary": "Получить список сезонов сериала и эпизодов в озвучке",
        "operationId": "get_tv_series_seasons_content_tv_series_seasons__get",
        "parameters": [
          {
            "required": false,
            "schema": {
              "title": "Url",
              "type": "string"
            },
            "name": "url",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Id",
              "type": "integer"
            },
            "name": "id",
            "in": "query"
          },
          {
            "required": false,
            "description": "Если параметр не указан, то возвращаются все озвучки",
            "schema": {
              "title": "Translation Id",
              "type": "string"
            },
            "name": "translation_id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Seasons"
                }
              }
            }
          }
        }
      }
    },
    "/content/tv_series/videos/": {
      "get": {
        "summary": "Get Tv Series Videos",
        "operationId": "get_tv_series_videos_content_tv_series_videos__get",
        "description": "Получить ссылку на видеофайл сериала по ссылке или id",
        "parameters": [
          {
            "schema": {
              "title": "Url",
              "type": "string"
            },
            "name": "url",
            "in": "query"
          },
          {
            "schema": {
              "title": "Id",
              "type": "string"
            },
            "name": "id",
            "in": "query"
          },
          {
            "schema": {
              "title": "Translation Id",
              "type": "string"
            },
            "name": "translation_id",
            "in": "query"
          },
          {
            "required": true,
            "schema": {
              "title": "Season Id",
              "type": "string"
            },
            "name": "season_id",
            "in": "query"
          },
          {
            "required": true,
            "schema": {
              "title": "Series Id",
              "type": "string"
            },
            "name": "series_id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MediaFile"
                }
              }
            }
          }
        }
      }
    },
    "/content/page/{page}": {
      "get": {
        "summary": "Get Content",
        "operationId": "get_content_content_page__page__get",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Page",
              "type": "integer"
            },
            "name": "page",
            "in": "path"
          },
          {
            "required": false,
            "style": "form",
            "explode": true,
            "description": "Sort by",
            "schema": {
              "title": "Filter",
              "type": "string",
              "default": "last",
              "enum": [
                "last",
                "popular",
                "soon",
                "watching"
              ]

            },
            "name": "filter",
            "in": "query"
          },
          {
            "required": false,
            "style": "form",
            "explode": true,
            "description": "Type of content",
            "schema": {
              "title": "Type",
              "type": "string",
              "default": "all",
              "enum": [
                "all",
                "film",
                "series",
                "cartoon",
                "anime"
              ]
            },
            "name": "type",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Data"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/content/category/page/{page}": {
      "get": {
        "summary": "Get Content By Categories",
        "operationId": "get_content_by_categories_content_category_page__page__get",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Page",
              "type": "integer"
            },
            "name": "page",
            "in": "path"
          },
          {
            "required": false,
            "schema": {
              "title": "Type",
              "type": "string",
              "default": "films",
              "enum": [
                "films",
                "series",
                "cartoons",
                "animation"
              ]
            },
            "name": "type",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Genre",
              "type": "string",
              "default": "any"
            },
            "name": "genre",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Year",
              "type": "integer"
            },
            "name": "year",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Data"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Data": {
        "title": "Data",
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "integer",
            "example": 2136
          },
          "type": {
            "title": "Type",
            "type": "string",
            "example": "Мультфильм"
          },
          "title": {
            "title": "Title",
            "type": "string",
            "example": "Рик и Морти"
          },
          "mirrorLessUrl": {
            "title": "Mirror Less Url",
            "type": "string",
            "example": "http://rd8j1em1zxge.org/cartoons/comedy/2136-rik-i-morti-2013.html"
          },
          "imageUrl": {
            "title": "Image Url",
            "type": "string",
            "example": "http://static.rezka.cloud/i/2022/8/24/f6275fb49917bza68n61e.jpg"
          },
          "year": {
            "title": "Year",
            "type": "string",
            "example": "2013 - ..."
          },
          "country": {
            "title": "Country",
            "type": "string",
            "example": "США"
          },
          "genre": {
            "title": "Genre",
            "type": "string",
            "example": "Комедии"
          },
          "status": {
            "title": "Status",
            "type": "string",
            "nullable": true,
            "example": "6 сезон, 9 серия"
          }
        }
      },
      "Concrete": {
        "title": "Concrete",
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "integer",
            "example": 2136
          },
          "url": {
            "title": "Mirror Less Url",
            "type": "string",
            "example": "http://rd8j1em1zxge.org/cartoons/comedy/2136-rik-i-morti-2013.html"
          },
          "type": {
            "title": "type",
            "type": "string",
            "example": "tv_series"
          },
          "title": {
            "title": "Title",
            "type": "string",
            "example": "Рик и Морти"
          },
          "description": {
            "title": "Description",
            "type": "string",
            "example": "Сюжет повествует о безудержных приключениях гениального ученого, страдающего от алкогольной зависимости Рика Санчеза и его наивного..."
          },
          "imageUrl": {
            "title": "Image url",
            "type": "string",
            "example": "http://static.rezka.cloud/i/2022/8/24/zb04639f399efcl66z13b.png"
          },
          "data": {
            "type": "object",
            "properties": {
              "rating": {
                "description": "Rating IMDB or Kinopoisk",
                "type": "string",
                "example": "IMDb: 9.1 (520 400) Кинопоиск: 9.00 (771 737)"
              },
              "slogan": {
                "description": "Slogan",
                "type": "string",
                "example": "Зачем семья, когда есть наука?"
              },
              "release": {
                "description": "Release date",
                "type": "string",
                "example": "2 декабря 2013 года"
              },
              "country": {
                "description": "Country of production",
                "type": "string",
                "example": "США"
              },
              "director": {
                "description": "Director",
                "type": "string",
                "example": "Уэсли Арчер, Пит Мишелс, Johnny Tesoro, Nathan Litz"
              },
              "genre": {
                "description": "Genre",
                "type": "string",
                "example": "Комедии, Фантастика, Приключения, Для взрослых, Мультсериалы, Зарубежные"
              },
              "quality": {
                "description": "Quality",
                "type": "string",
                "example": "720p"
              },
              "age": {
                "description": "Age limit",
                "type": "string",
                "example": "18+ только для взрослых"
              },
              "time": {
                "description": "Time of the episode",
                "type": "string",
                "example": "00:22"
              },
              "actors": {
                "description": "Actors",
                "type": "string",
                "example": "Джастин Ройланд, Крис Парнелл, Спенсер Грэммер, Сара Чок, Кари Уолгрен, Дэн Хармон, Морис ЛаМарш, Том Кенни, Брэндон Джонсон, Райан Ридли"
              }
            }
          },
          "translations_id": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ConcreteTranslation"
            }
          }
        }
      },
      "ConcreteTranslation": {
        "required": [
          "country"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "HDrezka Studio"
          },
          "id": {
            "type": "string",
            "example": "111"
          }
        }
      },
      "Translations": {
        "type": "object",
        "properties": {
          "HDrezka Studio":{
            "type": "string",
            "example": 111
          },
          "TVShows":{
            "type": "string",
            "example": 232
          },
          "GurGurStudio":{
            "type": "string",
            "example": 465
          }
        }
      },
      "MediaFile": {
